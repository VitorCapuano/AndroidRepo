@model IEnumerable<WebApplication1.Models.Carro>
@{
    ViewBag.Title = "Index";
}

<h2>Lista de Carros</h2>

<table class="table table-bordered">
    <tr>
        <th>@Html.DisplayNameFor(m => m.Id)</th>
        <th>@Html.DisplayNameFor(m => m.Modelo)</th>
        <th>@Html.DisplayNameFor(m => m.Ano)</th>
        <th>@Html.DisplayNameFor(m => m.Placa)</th>
        <th></th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Modelo</td>
            <td>@item.Ano</td>
            <td>@item.Placa</td>
            <td>@Html.ActionLink("Editar", "Edit", new { id = item.Id })</td>
        </tr>
    }

</table>

